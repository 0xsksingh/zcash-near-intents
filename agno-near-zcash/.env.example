# NEAR Account Configuration
NEAR_ACCOUNT_FILE=./account_file.json  # Path to your NEAR account file

# ZEC Agent Configuration
ZEC_DEFAULT_PRIVACY_LEVEL=shielded  # Options: 'transparent', 'shielded'
ZEC_AUTO_SHIELD=true                # Automatically shield incoming ZEC
ZEC_MEMOS_ENABLED=true              # Include transaction memos

# NEAR Intents Configuration
NEAR_DEPOSIT_AMOUNT=1.0            # Default amount to deposit for intent operations
SOLVER_BUS_URL=https://solver-relay-v2.chaindefuser.com/rpc  # NEAR Intents Solver Bus URL

# Agno Configuration
OPENAI_API_KEY=sk-your-openai-api-key  # Required for Agno agents
MODEL_NAME=gpt-4o                      # Model to use for agents

# Optional Configurations
ENABLE_DEBUG_LOGGING=false        # Enable verbose debug logging
MAX_AGENTS_PER_TEAM=4             # Maximum number of agents in a team 